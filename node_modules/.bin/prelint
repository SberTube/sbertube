#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ngdev/Desktop/sbertube/sbertube-2/node_modules/.pnpm/prettier-eslint-config@1.0.7/node_modules/prettier-eslint-config/dist/node_modules:/home/ngdev/Desktop/sbertube/sbertube-2/node_modules/.pnpm/prettier-eslint-config@1.0.7/node_modules/prettier-eslint-config/node_modules:/home/ngdev/Desktop/sbertube/sbertube-2/node_modules/.pnpm/prettier-eslint-config@1.0.7/node_modules:/home/ngdev/Desktop/sbertube/sbertube-2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ngdev/Desktop/sbertube/sbertube-2/node_modules/.pnpm/prettier-eslint-config@1.0.7/node_modules/prettier-eslint-config/dist/node_modules:/home/ngdev/Desktop/sbertube/sbertube-2/node_modules/.pnpm/prettier-eslint-config@1.0.7/node_modules/prettier-eslint-config/node_modules:/home/ngdev/Desktop/sbertube/sbertube-2/node_modules/.pnpm/prettier-eslint-config@1.0.7/node_modules:/home/ngdev/Desktop/sbertube/sbertube-2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier-eslint-config/dist/index.js" "$@"
else
  exec node  "$basedir/../prettier-eslint-config/dist/index.js" "$@"
fi
